{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "outDir": "dist",
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
    },
    "include": [
        "*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}